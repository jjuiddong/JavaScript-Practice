var member = ['egoing', 'k8805', 'sorialgi']
var arr = [1, 2, 3, 4, 5];
alert(arr.length);

var li = ['a', 'b', 'c', 'd', 'e'];
li.push('f');
li = li.concat(['f', 'g']);
li.unshift('z');
li.splice(2, 0, 'B');
li.shift();
li.pop();
li.sort();
li.reverse();

var grades = {'egoing': 10, 'k8805': 6, 'sorialgi': 80};
alert(grades['sorialgi']);
alert(grades.sorialgi);

for(key in grades) {
    document.write("key : "+key+" value : "+grades[key]+"<br />");
}



