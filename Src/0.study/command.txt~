
for (var i=0; i < 10; ++i)
{
	for (var k=0; k < 10; ++k)
	{
		document.write('hello world'+ i + k '<br/>' );
	}
}
-----------------------------------------------------------------
function numbering(){
    i = 0;
    while(i < 10){
        document.write(i);
        i += 1;
    }   
}
